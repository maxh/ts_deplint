{
  "name": "@ts_deplint/cli",
  "version": "0.0.5",
  "bin": "lib/index.js",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc",
    "dev": "yarn build && node lib/index.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxh/ts_deplint.git"
  },
  "author": "Max Heinritz",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "typescript": "^5.4.3"
  },
  "optionalDependencies": {
    "@ts_deplint/cli-darwin-arm64": "0.0.5",
    "@ts_deplint/cli-darwin-x64": "0.0.5",
    "@ts_deplint/cli-linux-arm64": "0.0.5",
    "@ts_deplint/cli-linux-x64": "0.0.5"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "ignorePatterns": [
      "lib/*"
    ],
    "root": true
  }
}
